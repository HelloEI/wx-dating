<!--点击查看相册-->
<view class="photo">
  <block wx:for="{{ userPhotos }}" wx:key="unique">
    <view data-index="{{ index }}" catchtap="showImg" class="photo-wrap">
      <image class="imgs" src="{{ item.img }}"></image>
      <image wx:if="{{ item.kind == 'other' }}" class="other-up" src="../../images/other-up.png"></image>
    </view>
  </block>
</view>
