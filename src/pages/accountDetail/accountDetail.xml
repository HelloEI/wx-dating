<!--交易详情-->
<view class="detail-wrap">
  <block wx:for="{{ info }}">
    <view class="d-w-wrap">
      <view class="left">
        <text>{{ item.kind }}</text>
        <text>{{ item.time }}</text>
      </view>
      <view class="right">
        <text>{{ item.money }}(元)</text>
        <text>{{ item.pay }}</text>
      </view>
    </view>
  </block>
</view>